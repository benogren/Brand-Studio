{
  "config_version": "1.0",
  "description": "Agent Engine deployment configuration for AI Brand Studio",
  "deployment_name": "brand-studio-agent",
  "resource_settings": {
    "min_instances": 0,
    "max_instances": 5,
    "cpu": "2",
    "memory": "4Gi"
  },
  "scaling_settings": {
    "auto_scaling": true,
    "scale_down_delay": "300s",
    "target_cpu_utilization": 0.7,
    "target_memory_utilization": 0.8
  },
  "environment_variables": {
    "GOOGLE_GENAI_USE_VERTEXAI": "1",
    "LOG_LEVEL": "INFO"
  },
  "timeout_settings": {
    "request_timeout": "600s",
    "idle_timeout": "300s"
  },
  "health_check": {
    "enabled": true,
    "check_interval": "30s",
    "timeout": "10s",
    "unhealthy_threshold": 3
  }
}
